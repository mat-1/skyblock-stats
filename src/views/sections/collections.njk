<ul>
{%- for coll in data.member.collections | sort(true, true, 'category') -%}
    <li class="tooltip" data-tooltip="{{ coll.xp }} xp" tabindex="1">
        {{ coll.name|clean }} <span class="coll-level{% if coll.level == skill.maxLevel %} coll-maxed{% endif %}">{{ skill.level }}</span>
    </li>
{%- endfor -%}
</ul>